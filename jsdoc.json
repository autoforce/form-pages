{
    "opts": {
        "template": "./node_modules/foodoc/template",
        "destination": "./docs",
        "tutorials": "./tutorials"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "sourceType": "module",
    "templates": {
        "applicationName": "FormPages",
        "linenums": true,
        "source": {
            "include": [
                "./src/"
            ],
            "includePattern": ".+\\.js(doc)?$",
            "excludePattern": "(^|\\/|\\\\)_"
        }
    }
}