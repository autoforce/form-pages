{
    "opts": {
        "template": "./node_modules/foodoc/template",
        "destination": "./docs",
        "tutorials": "./tutorials"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "sourceType": "module",
    "templates": {
        "applicationName": "FormPages",
        "linenums": true,
        "systemName": "FormPages",
        "systemSummary": "A simple way to paginate your forms<br><a href=''>GitHub repo</a><br> <a class='github-button' href='https://github.com/autoforce/form-pages/fork' data-icon='octicon-repo-forked' data-size='large' aria-label='Fork formPages on GitHub'>Fork us on GitHub</a>",
        "copyright": "",
        "footer": "Made with â™¥ by <a target='blank' href='https://autoforce.com/?utm_source=github&utm_medium=footer&utm_campaign=repo-formpages&utm_content=docs'>AutoForce</a>",
        "collapseSymbols": false,
        "source": {
            "include": [
                "./src/"
            ],
            "includePattern": ".+\\.js(doc)?$",
            "excludePattern": "(^|\\/|\\\\)_"
